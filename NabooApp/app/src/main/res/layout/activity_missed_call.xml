<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#151414">

    <TextView
        android:id="@+id/friend_book_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/takeout_2_back_margin_left"
        android:layout_marginTop="@dimen/takeout_2_back_margin_left"
        android:text="未接电话"
        android:textColor="#fff"
        android:textSize="@dimen/takeout_1_base_text_size" />

    <ListView
        android:id="@+id/missed_call_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/takeout_2_back_margin_left"/>
</LinearLayout>
