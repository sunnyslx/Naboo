<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:layout_marginTop="@dimen/nav_header_individua_menu_margin_top"
        android:id="@+id/individua_menu"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nav_header_layout_height"
        android:orientation="horizontal">
        <ImageView
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:layout_width="@dimen/nav_header_ImageView_layout_width"
            android:layout_height="@dimen/nav_header_ImageView_layout_height"
            android:layout_gravity="center"
            android:clickable="false"
            android:src="@drawable/individua_center_icon"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:layout_gravity="center"
            android:textSize="@dimen/nav_header_textsize"
            android:textColor="#ffffff"
            android:clickable="false"
            android:text="个人中心"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/contacts_menu"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nav_header_layout_height"
        android:orientation="horizontal">
        <ImageView
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:layout_width="@dimen/nav_header_ImageView_layout_width"
            android:layout_height="@dimen/nav_header_ImageView_layout_height"
            android:clickable="false"
            android:layout_gravity="center"
            android:src="@drawable/contacts_btn_icon"/>

        <TextView
            android:clickable="false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:textColor="#ffffff"
            android:layout_gravity="center"
            android:textSize="@dimen/nav_header_textsize"
            android:text="通讯录"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/set_menu"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nav_header_layout_height"
        android:orientation="horizontal">
        <ImageView
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:layout_width="@dimen/nav_header_ImageView_layout_width"
            android:layout_height="@dimen/nav_header_ImageView_layout_height"
            android:layout_gravity="center"
            android:src="@drawable/set_btn_icon"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:layout_gravity="center"
            android:textColor="#ffffff"
            android:textSize="@dimen/nav_header_textsize"
            android:text="设置"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/ear_menu"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/nav_header_layout_height"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="@dimen/nav_header_ImageView_layout_width"
            android:layout_height="@dimen/nav_header_ImageView_layout_width1"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:src="@drawable/ear_btn_icon" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginLeft="@dimen/nav_header_ImageView_margin_left"
            android:text="EarEQ"
            android:textColor="#ffffff"
            android:textSize="@dimen/nav_header_textsize" />
    </LinearLayout>

</LinearLayout>
